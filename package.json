{
  "name": "react-mobx-todo",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "material-ui": "^0.20.2",
    "mobx-react-devtools": "^6.0.2",
    "react-scripts": "1.1.4",
    "shortid": "^2.2.12",
    "todomvc-app-css": "^2.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "node-sass": "^4.9.3",
    "node-sass-chokidar": "^1.3.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "scripts": {
    "start": "yarn sass:build && react-scripts start",
    "build": "yarn sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true"
  }
}
